module Click_LPG_adc

include Click_LPG_types

sub procedure lpg_adcInit() 
sub function  lpg_readADC() as uint32_t 

implements

sub procedure lpg_adcInit() 

    ADC1_Init() 
    ADC_Set_Input_Channel(_ADC_CHANNEL_15) 

end sub

sub function  lpg_readADC() as uint32_t 
dim
    readADC as uint32_t

    readADC = ADC1_Read(15) 
    result = readADC 

end sub

end.