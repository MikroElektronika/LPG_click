unit Click_LPG_adc;

uses Click_LPG_types;

procedure lpg_adcInit(); 
function  lpg_readADC() :  uint32_t; 

implementation

procedure lpg_adcInit(); 
begin

    ADC1_Init(); 

end;

function  lpg_readADC() :  uint32_t; 
var
    readADC : uint32_t;
begin

    readADC := ADC1_Read(8); 
    result := readADC; 

end;

end.